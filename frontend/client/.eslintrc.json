{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint-config-next/core-web-vitals",
        "eslint-config-standard"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "semi": ["error", "always"],
        "quotes": ["error", "double"]
    }
}
